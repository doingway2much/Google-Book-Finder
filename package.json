{
  "name": "google-book-finder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^5.0.0",
    "dot-env": "0.0.1",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongodb": "^3.3.3",
    "mongoose": "^5.7.5",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run start:prod || yarn run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"yarn run client\"",
    "client": "cd client && yarn run start",
    "install": "cd client && yarn install",
    "build": "cd client && yarn run build",
    "heroku-postbuild": "yarn run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "server.js",
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doingway2much/google-book-finder.git"
  },
  "author": "Seth Randell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/doingway2much/google-book-finder/issues"
  },
  "homepage": "https://github.com/doingway2much/google-book-finder#readme"
}
